begin(factorial_recursivo)

int fx fibonacci(int n) {
    var int n2, res, res2;
    if(n<=1){
        res=n;
    } else {
        n = n - 1;
        n2 = n - 2;
        res = fibonacci(n);
        res2 = fibonacci(n2);
        res = res + res2;
        output(res);
    }
    return res;
    
}

main(){
    var int n, m;
    n = 20;
    m = fibonacci(n);
    output("El fibonacci de");
    output(n);
    output("es ");
    output(m);
}

end(factorial_recursivo)