begin(factorial_recursivo)

int fx factorial(int x){
    return x;
}

dec fx hola(dec x){
    return x;
}

string fx bye(string x){
    return x;
}

main(){
    var int n, m;
    var dec t, p;
    var string a, b;
    t = 2.3;
    n = 6;
    a = "bye";
